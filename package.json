{
  "name": "hubot-dingtalk-bot",
  "version": "1.5.0-b14",
  "description": "A hubot script that communicates with dingtalk",
  "main": "dingtalk.js",
  "scripts": {
    "test": "jest -i --forceExit --detectOpenHandles",
    "test:coverage": "jest -i --coverage --forceExit --detectOpenHandles"
  },
  "keywords": [
    "hubot",
    "robot",
    "adapter",
    "dingtalk"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:anjia0532/hubot-dingtalk-bot.git"
  },
  "bugs": {
    "url": "https://github.com/anjia0532/hubot-dingtalk-bot/issues"
  },
  "homepage": "https://github.com/anjia0532/hubot-dingtalk-bot",
  "author": "AnJia <anjia0532@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "hubot": "^3.3.2"
  },
  "dependencies": {
    "dingtalk-robot-sdk": "^1.0.1"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "got": "^11.1.3",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "scoped-http-client": "^0.11.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/helpers/",
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "clover"
    ],
    "coverageDirectory": "./test-results",
    "collectCoverage": true
  }
}
